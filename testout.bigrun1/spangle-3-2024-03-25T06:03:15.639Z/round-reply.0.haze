
fun model, action ->
  case action
  | AddBooking(new_booking)
  | .booking_id.exported = new_booking
  | ModifyBooking(modified_booking) => ??
  | DeleteBooking(deleted_booking_id)
  | ClearBookings
  | SendInvite(invite)
  | ClearInvites
  | RejectInvite(reject_invite)
  | ChangeFilter(new_filter)
  | ChangeView(new_view)
  | UpdateFormState(new_state)
  | UpdateFormText(new_text) =>
    set .form_state to new_state
    set .form_text to new_text
  end