# TODO MVU: Handle TODO actions to update the app model #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | Action.Increment => {model with count = model.count + 1}
  | Action.Decrement => {model with count = model.count - 1}
  | Action.Reset => {model with count = 0}
  end
in
