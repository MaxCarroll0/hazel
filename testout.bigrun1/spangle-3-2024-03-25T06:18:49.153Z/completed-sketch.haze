# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  
fun model, action ->
  case action
  | AddRoom(room) => add_room(model, room)
  | RemoveRoom(room) => remove_room(model, room)
  | BookRoom(room, slot) => book_room(model, room, slot)
  | CancelBooking(room, slot) => cancel_booking(model, room, slot)
  end
in
