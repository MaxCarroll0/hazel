fun model, action ->
    case action
    | PlaySong(id) => (get_songs(model), Playing(id))
    | PauseCurrentSong => (get_songs(model), PausedOn(get_current_song(model)))
    | RemoveSong(id) => (List.filter(fun song -> song != id, get_songs(model)), get_state(model))
    | AddSong(id) => (id::get_songs(model), get_state(model)) 
    end
