fun model, action ->
  let grid, emoji, available_emojis = model in
  case action
  | SelectEmoji(e) => (grid, e, available_emojis)
  | StampEmoji(row, col) => (updateGrid(grid, row, col, emoji), emoji, available_emojis)
  | ClearCell(row, col) => (updateGrid(grid, row, col, ""), emoji, available_emojis)
  | ClearGrid => (clearGrid(grid), emoji, available_emojis)
  | FillRow(row) => (fillRowInGrid(grid, row, emoji), emoji, available_emojis)
  end