# TODO MVU: Handle TODO actions to update the app model #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | AddTodo => (Model.init, add(model))
  | RemoveTodo(i) => (Model.init, remove(i, model))
  | ToggleTodo(i) => (Model.init, toggle(i, model))
  | UpdateBuffer(s) => (s, model)
  end
in
