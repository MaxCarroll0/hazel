# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun model, action ->
    case action
    | Add(track) => update_add_action(model, track)
    | Delete(track) => update_delete_action(model, track)
    | MoveUp(track) => update_move_up_action(model, track)
    | MoveDown(track) => update_move_down_action(model, track)
    end
in