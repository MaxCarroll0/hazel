fun model, action ->
  case action
  | Action.SetName(new_name) =>
    { model with name = new_name }
  | Action.SetBookings(new_bookings) =>
    { model with bookings = List.append(model.bookings,new_bookings) }
  | Action.AddBooking(new_booking) =>
    { model with bookings = new_booking::model.bookings }
  | Action.RemoveBooking(booking_index) =>
    { model with bookings = List.remove_at(booking_index,model.bookings) }
  | Action.ClearBookings =>
    { model with bookings = [] }
  end