# TODO MVU: Handle TODO actions to update the app model #

let update: (Model, Action) -> Model =
  fun (model, action) ->
  case action
  | AddTodo => add(model)
  | RemoveTodo(id) => {model with todos = remove(id, model.todos)}
  | ToggleTodo(id) => {model with todos = toggle(id, model.todos)}
  end
in
