fun (model, action) ->
  let (grid, sel_emoji, emoji_list) = model in
  case action
  | SelectEmoji(new_emoji) => (grid, new_emoji, emoji_list)
  | StampEmoji((row, col)) => (updateGrid(grid, row, col, sel_emoji), sel_emoji, emoji_list)
  | ClearCell((row, col)) => (updateGrid(grid, row, col, ""), sel_emoji, emoji_list)
  | ClearGrid => (clearGrid(grid), sel_emoji, emoji_list)
  | FillRow(row) => (fillRowInGrid(grid, row, sel_emoji), sel_emoji, emoji_list)
  end