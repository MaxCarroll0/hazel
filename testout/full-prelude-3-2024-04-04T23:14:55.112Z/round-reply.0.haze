
fun model, action ->
  let (form_data, bookings, id) = model in
  case action
    | AddBooking(user, weekday, time_of_day) =>
      if bookingExists(model, ((weekday, time_of_day), user, id))
      then model
      else ((weekday, time_of_day), user, id+1)::bookings, id+1
    | CancelBooking(user, booking_id) =>
      (form_data, rm_booking(user, booking_id, bookings), id)
    | ClearBookings =>
      (form_data, [], id)
  end