
fun model, action ->
  let (formData, bookings, nextId) = model in
  case action
    | AddBooking(user, weekday, timeOfDay) =>
        if bookingExists(model, ((weekday, timeOfDay), user, nextId)) then
          model
        else
          ((weekday, timeOfDay), user, nextId + 1)::bookings, nextId + 1
    | CancelBooking(user, id) => 
        (formData, rm_booking(user, id, bookings), nextId)
    | ClearBookings => 
        (formData, [], nextId)
  end