fun (model, action) -> 
  case action
    | SelectEmoji(emoji) => (model[0], emoji, model[2])
    | StampEmoji((row, col)) => (update_grid_with_emoji(model[0], (row, col), model[1]), model[1], model[2])
    | ClearCell((row, col)) => (clear_cell_in_grid(model[0], (row, col)), model[1], model[2])
    | ClearGrid => (initialize_grid(), model[1], model[2]) 
    | FillRow(row) => (fill_row_in_grid(model[0], row, model[1]), model[1], model[2]) 
    end