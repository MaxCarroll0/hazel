# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun (playList, playListAction) ->
  case playListAction
  | PlaySong(id) => updatePlayList(playList, id)
  | PauseCurrentSong => pauseCurrentSong(playList)
  | RemoveSong(id) => removeSong(playList, id)
  | AddSong(id) => addSongToPlayList(playList, id)
  end
in