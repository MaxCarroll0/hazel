fun (model, action) ->
  case action
    | AddTodo =>
      let todo = ? in
      (model, todo :: model)
    | RemoveTodo(index) =>
      let new_model = ? in
      new_model
    | ToggleTodo(index) =>
      let new_model = ? in
      new_model
    | UpdateBuffer(string) =>
      let new_model = ? in
      new_model
    end end