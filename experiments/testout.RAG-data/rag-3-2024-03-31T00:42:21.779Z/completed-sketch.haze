# Update Password Strength Checker app model based on an action #

let update: (Model, Action) -> Model =
  fun (model, action) ->
    case action
    | AddTodo => add_logic(model)
    | RemoveTodo(index) => remove_logic(model, index)
    | ToggleTodo(index) => toggle_logic(model, index)
    | UpdateBuffer(text) => update_buffer_logic(model, text)
    end
in