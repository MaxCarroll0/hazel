# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  fun m, a ->
case a
| AddBooking(bookingData) => if bookingExists(m, bookingData) then m else addBooking(m, bookingData)
| UpdateForm(formState) => updateForm(m, formState)
| DeleteBooking(bookingId) => deleteBooking(m, bookingId)
end
in
