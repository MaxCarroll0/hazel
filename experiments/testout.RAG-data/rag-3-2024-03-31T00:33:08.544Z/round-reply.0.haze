fun (playList, action) -> 
    case action
      | PlaySong(id) => let songs, state = playList in (songs, Playing(id))
      | PauseCurrentSong => let songs, _ = playList in (songs, NoSongSelected)
      | RemoveSong(id) => let songs, state = playList in (List.remove(songs, id), state)
      | AddSong(id) => let songs, state = playList in (id::songs, state)
      end