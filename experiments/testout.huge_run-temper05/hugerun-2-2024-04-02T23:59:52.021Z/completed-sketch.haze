# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun (model, action) ->
  case action
    | SelectEmoji(emoji) => (emoji, model[1])
    | StampEmoji(row, col) => updateGrid(model[0], row, col, model[0])
    | FillRow(row) => fillRow(model[0], row, model[0]) 
  end
in
