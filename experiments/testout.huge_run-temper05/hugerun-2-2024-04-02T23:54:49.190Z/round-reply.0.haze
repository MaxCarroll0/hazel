fun model, action ->
  case action
  | AddBooking(booking) => {model with bookings = booking::model.bookings}
  | RemoveBooking(bookingID) => {model with bookings = rm_booking(model.user, bookingID, model.bookings)}
  | UpdateUser(user) => {model with user = user}
  | UpdateFormState(formState) => {model with formState = formState}
  end