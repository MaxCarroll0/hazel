fun model, action ->
  case action
  | SelectEmoji(emoji) => (fst(model), emoji, trd(model))
  | StampEmoji((row, col)) => (Grid.stamp(row, col, snd(model), fst(model)), snd(model), trd(model))
  | ClearCell((row, col)) => (Grid.clear(row, col, fst(model)), snd(model), trd(model))
  | ClearGrid => (Grid.empty(), "", trd(model))
  | FillRow(row) => (Grid.fill(row, snd(model), fst(model)), snd(model), trd(model))
  end