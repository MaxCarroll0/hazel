fun model, action ->
  let (grid, current_emoji, emoji_list) = model in
  case action
  | SelectEmoji(emoji) => (grid, emoji, emoji_list)
  | StampEmoji((row, col)) => (update_grid_with_emoji(grid, row, col, current_emoji), current_emoji, emoji_list)
  | ClearCell((row, col)) => (clear_cell_in_grid(grid, row, col), current_emoji, emoji_list)
  | ClearGrid => (empty_grid(), "", emoji_list)
  | FillRow(row) => (fill_row_in_grid(grid, row, current_emoji), current_emoji, emoji_list)
  end