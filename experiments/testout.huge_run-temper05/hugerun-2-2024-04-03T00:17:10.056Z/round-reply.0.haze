fun model, action ->
    case action
    | Click(row, col) => {model with grid = updateGrid(model.grid, row, col, model.currentEmoji)}
    | Select(emoji) => {model with currentEmoji = emoji}
    | Fill(row) => {model with grid = fillRowInGrid(model.grid, row, model.currentEmoji)}
    | Clear => {model with grid = clearGrid(model.grid)}
    end