# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  
fun model, action ->
  case action
  | CheckIn(guest) => { model with checkIns = guest::model.checkIns }
  | CheckOut(guest) => { model with checkOuts = guest::model.checkOuts }
  | NoOp => model
  end
in
