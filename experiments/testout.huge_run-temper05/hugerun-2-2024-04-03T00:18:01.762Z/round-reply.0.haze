fun model, action ->
  case action
  | SetEmoji(emoji) => {model with current_emoji = emoji}
  | FillRow(row) => {model with grid = fillRowInGrid(model.grid, row, model.current_emoji)}
  | Clear => {model with grid = clearGrid(model.grid)}
  | UpdateCell(row, col) => {model with grid = updateGrid(model.grid, row, col, model.current_emoji)}
  end