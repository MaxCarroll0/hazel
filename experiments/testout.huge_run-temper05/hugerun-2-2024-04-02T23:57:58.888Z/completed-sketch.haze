# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun (model, action) ->
  case action
  | SelectEmoji(emoji) => let (grid, _) = model in (grid, emoji)
  | StampEmoji(row, col) => let (grid, emoji) = model in updateGrid(grid, row, col, emoji)
  | FillRow(row) => model end
in
