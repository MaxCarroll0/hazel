fun model, action ->
  case action
  | SelectEmoji(emoji) => (fst(fst(model)), emoji, snd(model))
  | StampEmoji((row, col)) => (Grid.stamp(row, col, snd(fst(model)), fst(model)), snd(fst(model)), snd(model))
  | ClearCell((row, col)) => (Grid.clear_cell(row, col, fst(model)), snd(fst(model)), snd(model))
  | ClearGrid => (Grid.clear(), snd(fst(model)), snd(model))
  | FillRow(row) => (Grid.fill_row(row, snd(fst(model)), fst(model)), snd(fst(model)), snd(model))
  end