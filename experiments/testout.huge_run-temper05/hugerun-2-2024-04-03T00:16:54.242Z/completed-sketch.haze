# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | AddEmoji(emoji) => {model with canvas = emoji :: model.canvas}
  | ClearCanvas => {model with canvas = []}
  | Undo => 
    case model.canvas 
    | [] => model
    | _::rest => {model with canvas = rest}
    end
  end
in
