fun model, action ->
  case action
  | Init => Model.init
  | SubmitForm(user, formState) =>
      let newBooking = Booking(user, formState) in
      let updatedBookings = model.bookings @ [newBooking] in
      { model with bookings = updatedBookings }
  | CancelBooking(user, bookingId) =>
      let updatedBookings = rm_booking(user, bookingId, model.bookings) in
      { model with bookings = updatedBookings }
  end