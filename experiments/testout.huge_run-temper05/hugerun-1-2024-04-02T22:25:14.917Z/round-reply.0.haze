fun model, action ->
  case action
  | SelectEmoji(emoji) => { model with selected_emoji = emoji }
  | Click(row, col) => { model with grid = updateGrid(model.grid, row, col, model.selected_emoji) }
  | Clear => { model with grid = clearGrid(model.grid) }
  | FillRow(row, emoji) => { model with grid = fillRowInGrid(model.grid, row, emoji) }
  end