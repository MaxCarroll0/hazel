fun (grid, current_emoji, emoji_list), action ->
  case action
  | SelectEmoji(emoji) => let (grid, _, emoji_list) = model in (grid, emoji, emoji_list)
  | StampEmoji((row, col)) => let (grid, current_emoji, emoji_list) = model in (update_grid(grid, row, col, current_emoji), current_emoji, emoji_list)
  | ClearCell((row, col)) => let (grid, current_emoji, emoji_list) = model in (update_grid(grid, row, col, ""), current_emoji, emoji_list)
  | ClearGrid => let (_, current_emoji, emoji_list) = model in (clear_grid(), current_emoji, emoji_list)
  | FillRow(row) => let (grid, current_emoji, emoji_list) = model in (fill_row(grid, row, current_emoji), current_emoji, emoji_list)
  end