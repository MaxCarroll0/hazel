fun m, a ->
  case a
  | AddBooking(u, wd, tod) => 
    let (form, bookings, id) = m in
    let new_booking = ((wd, tod), u, id + 1) in
    let new_bookings = new_booking::bookings in
    ((wd, tod), u), new_bookings, id + 1
  | CancelBooking(u, id) => 
    let (form, bookings, _) = m in
    let new_bookings = rm_booking(u, id, bookings) in
    form, new_bookings, id
  | ClearBookings => 
    initFormState, [], 0
  end