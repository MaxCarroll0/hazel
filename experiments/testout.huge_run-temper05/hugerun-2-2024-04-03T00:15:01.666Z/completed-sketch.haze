# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
    case action
    | Draw(point) => { model with points = point :: model.points }
    | Clear => { model with points = [] }
    | Undo => { model with points = List.tl(model.points) }
    | Redo => model  # Assuming no redo functionality
    end
in
