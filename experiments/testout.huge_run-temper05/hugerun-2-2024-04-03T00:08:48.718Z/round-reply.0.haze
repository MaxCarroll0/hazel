fun (model, action) ->
  case action
  | SelectEmoji(emoji) => (model.1, emoji, model.3)
  | StampEmoji((row, col)) => (Grid.stamp(model.1, row, col, model.2), model.2, model.3)
  | ClearCell((row, col)) => (Grid.clear_cell(model.1, row, col), model.2, model.3)
  | ClearGrid => (Grid.clear(), model.2, model.3)
  | FillRow(row) => (Grid.fill_row(model.1, row, model.2), model.2, model.3)
  end