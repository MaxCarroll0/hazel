# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | SelectEmoji(emoji) => (emoji, model[1])
  | StampEmoji(row, col) => updateGrid(model, row, col, model[0])
  | FillRow(row) => fillRow(model, row, model[0])
  end
in
