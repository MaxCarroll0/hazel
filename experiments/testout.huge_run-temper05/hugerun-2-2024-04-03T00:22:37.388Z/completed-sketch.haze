# Update Password Strength Checker app model based on an action #

let update: (Model, Action) -> Model =
  fun (model, action) ->
  case action
  | AddTodo => addTodo(model)
  | RemoveTodo(index) => removeTodo(model, index)
  | ToggleTodo(index) => toggleTodo(model, index)
  | UpdateBuffer(str) => updateBuffer(model, str)
  end
in