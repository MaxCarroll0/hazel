
fun model, action ->
  case action
  | Click(row, col) => {model with grid = updateGrid(model.grid, row, col, model.selected_emoji)}
  | Select(emoji) => {model with selected_emoji = emoji}
  | Clear => {model with grid = clearGrid(model.grid)}
  | Fill => {model with grid = fillRowInGrid(model.grid, model.current_row, model.selected_emoji)}
  | _ => model
  end
in
update(model_init, Click(1,1))