fun (playList, action) ->
  case action
  | PlaySong(id) => (List.filter(fun x -> x != id, get_songs(playList)), Playing(id))
  | PauseCurrentSong =>
      case get_state(playList) 
      | Playing(id) => (get_songs(playList), PausedOn(id))
      | _ => playList
      end
  | RemoveSong(id) => (List.filter(fun x -> x != id, get_songs(playList)), get_state(playList))
  | AddSong(id) => (id::get_songs(playList), get_state(playList))
  end