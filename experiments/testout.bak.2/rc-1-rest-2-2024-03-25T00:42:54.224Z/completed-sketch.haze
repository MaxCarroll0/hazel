# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun(model, action) ->
  case action 
    | AddSong(song) => addSongToPlaylist(model, song)
    | ClearPlaylist => clearPlaylist(model)
    | RemoveSong(songId) => removeSongFromPlaylist(model, songId)
    | MoveSong(oldIndex, newIndex) => moveSongInPlaylist(model, oldIndex, newIndex)
  end
in