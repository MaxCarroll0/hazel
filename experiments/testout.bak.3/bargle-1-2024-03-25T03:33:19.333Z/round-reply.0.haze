fun model, action ->
  case action
  | SelectEmoji(e) => (fst3(model), e, third3(model))
  | StampEmoji((r, c)) => (updateGrid(fst3(model), r, c, snd3(model)), snd3(model), third3(model))
  | ClearCell((r, c)) => (updateGrid(fst3(model), r, c, ""), snd3(model), third3(model))
  | ClearGrid => (clearGrid(fst3(model)), snd3(model), third3(model))
  | FillRow(row) => (fillRowInGrid(fst3(model), row, snd3(model)), snd3(model), third3(model))
  end
