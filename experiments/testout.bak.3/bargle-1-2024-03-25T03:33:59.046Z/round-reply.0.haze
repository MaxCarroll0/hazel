fun (model, action) ->
    case action
    | SelectEmoji(emoji) => (fst(model), emoji, thd(model))
    | StampEmoji((row, col)) => (insert_emoji(snd(model), row, col), snd(model), thd(model))
    | ClearCell((row, col)) => (clear_cell(fst(model), row, col), snd(model), thd(model))
    | ClearGrid => (init_grid(), snd(model), thd(model))
    | FillRow(row) => (fill_row(fst(model), row, snd(model)), snd(model), thd(model))
    end