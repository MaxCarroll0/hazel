fun model, action ->
  case action
  | SelectEmoji(emoji) => (model.0, emoji, model.2)
  | StampEmoji(row, col) => (updateGrid(model.0, row, col, model.1), model.1, model.2)
  | ClearCell(row, col) => (updateGrid(model.0, row, col, ""), model.1, model.2)
  | ClearGrid => (clearGrid(model.0), model.1, model.2)
  | FillRow(row) => (fillRowInGrid(model.0, row, model.1), model.1, model.2)
  end