# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
    | AddBooking(user, weekday, time) => addBooking(model, user, weekday, time)
    | RemoveBooking(id) => removeBooking(model, id)
    | UpdateForm(form) => updateForm(model, form)
    end
in
