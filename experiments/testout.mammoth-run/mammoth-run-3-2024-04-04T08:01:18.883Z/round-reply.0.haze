fun model, action ->
  case action
  | AddSong(id) => 
    let songs = get_songs(model) in
    let updated_songs = List.append(songs, [id]) in
    PlayList(updated_songs, get_state(model))
  | RemoveSong(id) =>
    let songs = get_songs(model) in
    let updated_songs = List.filter(fun song_id -> song_id != id, songs) in
    PlayList(updated_songs, get_state(model))
  | ChangeState(new_state) => PlayList(get_songs(model), new_state)
  end