# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun (playList, action) ->
  case action
  | AddSong(song) => PlayList.addSong(playList, song)
  | RemoveSong(song) => PlayList.removeSong(playList, song)
  | Play => PlayList.play(playList)
  | Pause => PlayList.pause(playList) end
in