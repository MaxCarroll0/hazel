# Update Password Strength Checker app model based on an action #

let update: (Model, Action) -> Model =
  
fun (model, action) ->
  case action
  | AddTodo => add_todo(model)
  | RemoveTodo(i) => remove_todo(model, i)
  | ToggleTodo(i) => toggle_todo(model, i)
  | UpdateBuffer(s) => update_buffer(model, s)
  end
in