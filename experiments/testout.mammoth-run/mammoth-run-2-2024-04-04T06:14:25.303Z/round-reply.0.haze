fun model, action ->
  let grid, selected, palette = model in
  case action
  | SelectEmoji(emoji) => (grid, emoji, palette)
  | StampEmoji((row, col)) => (updateGrid(grid, row, col, selected), selected, palette)
  | ClearCell((row, col)) => (updateGrid(grid, row, col, ""), selected, palette)
  | ClearGrid => (clearGrid(grid), selected, palette)
  | FillRow(row) => (fillRowInGrid(grid, row, selected), selected, palette)
  end