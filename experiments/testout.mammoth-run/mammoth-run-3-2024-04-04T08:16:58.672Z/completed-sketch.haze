# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun model, action ->
    case action
    | AddPlaylist(name) => addPlaylist(name, model)
    | RemovePlaylist(name) => removePlaylist(name, model)
    | UpdatePlaylist(name, songs) => updatePlaylist(name, songs, model)
    end
in