fun (model, action) ->
    case action
      | AddBooking(user, weekday, time) => 
        if bookingExists(model, (time, user, getBookingId(model))) then model
        else addBooking(model, (time, user, getBookingId(model)))
      | RemoveBooking(bookingId) =>
        removeBooking(model, bookingId)
      | UpdateBooking(bookingId, newTime) =>
        updateBooking(model, bookingId, newTime) 
    end