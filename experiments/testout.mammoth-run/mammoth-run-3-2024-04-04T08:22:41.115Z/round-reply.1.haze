fun (model, action) ->
    case action
      | AddBooking(user, weekday, time) => 
        if bookingExists(model, (time, user, getBookingId(model))) then model
        else addBooking(model, (time, user, incrementBookingId(model)))
      | RemoveBooking(id) =>
        removeBooking(model, id)
      | UpdateBooking(id, newTime) =>
        updateBooking(model, id, newTime) 
    end