fun model, action ->
case action
| AddBooking(user, weekday, timeOfDay) => let newBooking = (weekday, timeOfDay, user, model.bookingID + 1) in (model.bookingFormData, model.currentBookings ++ [newBooking], model.bookingID + 1)
| CancelBooking(user, bookingID) => let newBookings = rm_booking(user, bookingID, model.currentBookings) in (model.bookingFormData, newBookings, model.bookingID)
| ClearBookings => (model.bookingFormData, [], model.bookingID)
end