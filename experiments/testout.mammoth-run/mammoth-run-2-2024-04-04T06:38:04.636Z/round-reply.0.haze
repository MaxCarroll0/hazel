fun model, action ->
    case action
    | SetEmoji(emoji) => { model with currentEmoji = emoji }
    | SetGrid(grid) => { model with grid = grid }
    | ClearGrid => { model with grid = clearGrid(model.grid) }
    | UpdateCell(row, col) => { model with grid = updateGrid(model.grid, row, col, model.currentEmoji) }
    | FillRow(row) => { model with grid = fillRowInGrid(model.grid, row, model.currentEmoji) }
    end