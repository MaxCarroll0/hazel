
fun model, action ->
  case action
  | SelectEmoji(emoji) => {model with selectedEmoji=emoji}
  | PaintCell(row, col) => {model with grid=updateGrid(model.grid, row, col, model.selectedEmoji)}
  | FillRow(row) => {model with grid=fillRowInGrid(model.grid, row, model.selectedEmoji)}
  | Clear => {model with grid=clearGrid(model.grid)}
  end