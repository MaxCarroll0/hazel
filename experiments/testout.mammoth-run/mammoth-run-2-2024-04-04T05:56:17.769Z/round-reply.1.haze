fun (model, action) ->
  case action
  | AddBooking((user, weekday, timeOfDay)) => 
     let (formData, bookings, nextId) = model in
     ((weekday, timeOfDay, user), (nextId, user, (weekday, timeOfDay))::bookings, nextId + 1)
  | CancelBooking((user, id)) => 
     let (formData, bookings, nextId) = model in
     (formData, List.filter(fun (bookingId, _, _) -> bookingId != id) bookings, nextId)
  | ClearBookings => 
     let (formData, _, nextId) = model in
     (formData, [], nextId)
  end