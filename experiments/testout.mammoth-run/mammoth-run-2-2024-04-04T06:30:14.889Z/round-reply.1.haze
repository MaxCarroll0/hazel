fun model, action ->
  case action
  | SelectEmoji(emoji) => (fst(model), emoji, snd(snd(model)))
  | StampEmoji((row, col)) => (Model.stamp(row, col, snd(model), fst(model)), snd(model), snd(snd(model)))
  | ClearCell((row, col)) => (Model.clear(row, col, fst(model)), snd(model), snd(snd(model)))
  | ClearGrid => (Model.clear_all(fst(model)), snd(model), snd(snd(model)))
  | FillRow(row) => (Model.fill_row(row, snd(model), fst(model)), snd(model), snd(snd(model)))
  end