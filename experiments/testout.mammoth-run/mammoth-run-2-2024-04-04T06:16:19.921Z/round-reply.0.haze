
fun model, action ->
  let (grid, selected_emoji, _) = model in
  case action
  | SelectEmoji(emoji) => (grid, emoji, _)
  | StampEmoji((row, col)) => (updateGrid(grid, row, col, selected_emoji), selected_emoji, _)
  | ClearCell((row, col)) => (updateGrid(grid, row, col, ""), selected_emoji, _)
  | ClearGrid => (clearGrid(grid), selected_emoji, _)
  | FillRow(row) => (fillRowInGrid(grid, row, selected_emoji), selected_emoji, _)
  end