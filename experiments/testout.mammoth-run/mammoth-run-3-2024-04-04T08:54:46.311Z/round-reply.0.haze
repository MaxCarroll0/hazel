fun (model, action) ->
  case action
  | SelectEmoji(emoji) => (emoji, model[1])
  | StampEmoji(row, col) => 
    let updatedGrid = updateGrid(model[0], row, col, model[0])
    in (model[0], updatedGrid)
  | FillRow(row) =>
    let updatedGrid = List.map (fun _ -> model[0]) model[0][row]
    in (model[0], updatedGrid :: List.remove_at(row, model[1]))
  end