# TODO MVU: Handle TODO actions to update the app model #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | AddTodo => add(model)
  | RemoveTodo(idx) => remove(idx, model)
  | ToggleTodo(idx) => toggle(idx, model)
  | UpdateBuffer(str) => update_buffer(str, model)
  end
in
