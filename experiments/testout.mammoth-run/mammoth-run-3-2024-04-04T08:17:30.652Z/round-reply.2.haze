fun (model, action) ->
    case action
    | AddBooking(user, weekday, time) => if !bookingExists(model, (time, user, 0)) then model else addBookingToModel(model, (time, user, 0))
    | RemoveBookingByID(id) => removeBookingFromModel(model, id)
    | UpdateBookingBuffer(string) => updateBookingBufferInModel(model, string)
    end