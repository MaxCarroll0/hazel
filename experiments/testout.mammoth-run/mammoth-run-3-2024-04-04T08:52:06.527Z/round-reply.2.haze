fun (model, action) ->
    case action
    | SelectEmoji(emoji) => let (grid, _) = model in (grid, [emoji])
    | StampEmoji(row, col) => let (grid, emoji) = model in updateGrid(grid, row, col, List.hd(emoji))
    | FillRow(row) => let (grid, emoji) = model in fillRow(grid, row, List.hd(emoji))
    end