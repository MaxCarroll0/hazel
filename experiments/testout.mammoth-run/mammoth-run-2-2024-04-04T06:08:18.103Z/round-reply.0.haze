
fun model, action ->
  case action
  | AddRoom(name) => { model with rooms = model.rooms @ [name] }
  | RemoveRoom(name) => { model with rooms = List.filter((!=) name, model.rooms) }
  | BookRoom(room, time) => { model with bookings = (room, time) :: model.bookings }
  | CancelBooking(room, time) => { model with bookings = List.filter(fun (r, t) -> r != room \/ t != time, model.bookings) }