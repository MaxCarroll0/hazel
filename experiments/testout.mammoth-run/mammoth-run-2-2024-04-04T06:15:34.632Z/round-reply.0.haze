fun (model, action) ->
  let (grid, selectedEmoji, emojis) = model in
  case action
  | SelectEmoji(newEmoji) => (grid, newEmoji, emojis)
  | StampEmoji((row, col)) => (updateGrid(grid, row, col, selectedEmoji), selectedEmoji, emojis)
  | ClearCell((row, col)) => (updateGrid(grid, row, col, ""), selectedEmoji, emojis)
  | ClearGrid => (clearGrid(grid), selectedEmoji, emojis)
  | FillRow(row) => (fillRowInGrid(grid, row, selectedEmoji), selectedEmoji, emojis)
  end