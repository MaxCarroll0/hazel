fun model, action ->
  case action
  | SelectEmoji(emoji) => (fst(model), emoji, snd(snd(model)))
  | StampEmoji((row, col)) => (updateGrid(fst(model), row, col, snd(model)), snd(model), snd(snd(model)))
  | ClearCell((row, col)) => (updateGrid(fst(model), row, col, ""), snd(model), snd(snd(model)))
  | ClearGrid => (clearGrid(fst(model)), snd(model), snd(snd(model)))
  | FillRow(row) => (fillRowInGrid(fst(model), row, snd(model)), snd(model), snd(snd(model)))
  end