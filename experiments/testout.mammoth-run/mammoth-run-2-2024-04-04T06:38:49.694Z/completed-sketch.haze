# Update the EmojiPaint app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
    case action
    | DrawEmoji(emoji, position) => Model.addEmoji(model, emoji, position)
    | ClearCanvas => Model.clear(model)
    | Undo => Model.undo(model)
    | Redo => Model.redo(model)
    end
in
