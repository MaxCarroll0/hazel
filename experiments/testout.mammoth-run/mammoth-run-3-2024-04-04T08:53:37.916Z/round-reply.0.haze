fun (model, action) ->
  case action
  | SelectEmoji(emoji) => (emoji, model[1])
  | StampEmoji(row, col) =>
    let updated_grid = updateGrid(model[0], row, col, model[0])
    in (updated_grid, model[1])
  | FillRow(row) =>
    let updated_grid = List.map((fun row -> fillRow(row, model[0])), model[0])
    in (updated_grid, model[1])
  end