fun model, action ->
  case action
  | AddBooking(user, weekday, timeOfDay) => 
      let newBooking = ((weekday, timeOfDay), user, model.2 + 1) in
      (model.0, model.1 :: newBooking, model.2 + 1)
  | CancelBooking(user, bookingID) => 
      let updatedBookings = rm_booking(user, bookingID, getBookings(model)) in
      (model.0, updatedBookings, model.2)
  | ClearBookings => (model.0, [], model.2)
  end