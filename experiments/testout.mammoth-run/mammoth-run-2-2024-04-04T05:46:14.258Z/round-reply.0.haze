fun model, action ->
  case action
  | AddBooking(user, weekday, time) => addBooking(model, user, weekday, time)
  | RemoveBooking(bookingId) => removeBooking(model, bookingId)
  | UpdateUser(user) => updateUser(model, user)
  | UpdateWeekday(weekday) => updateWeekday(model, weekday)
  | UpdateTime(time) => updateTime(model, time)
  end