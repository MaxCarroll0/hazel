
fun model, action ->
  let grid, currentEmoji, _ = model in
  case action
  | SelectEmoji(emoji) => (grid, emoji, [])
  | StampEmoji((row, col)) => (updateGrid(grid, row, col, currentEmoji), currentEmoji, [])
  | ClearCell((row, col)) => (updateGrid(grid, row, col, ""), currentEmoji, [])
  | ClearGrid => (clearGrid(grid), currentEmoji, [])
  | FillRow(row) => (fillRowInGrid(grid, row, currentEmoji), currentEmoji, [])
  end