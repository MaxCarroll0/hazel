fun model, action ->
  case action
  | AddSong(song_id) => {model with songs = song_id::get_songs(model)}
  | RemoveSong(song_id) => {model with songs = List.filter(fun id -> id != song_id, get_songs(model))}
  | Play => {model with state = Playing}
  | Pause => {model with state = Paused}
  | Stop => {model with state = Stopped}
  end