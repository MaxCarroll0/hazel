fun (model, action) ->
  case action
  | AddBooking(user, weekday, timeOfDay) => addBooking(model, (user, weekday, timeOfDay))
  | CancelBooking(user, bookingID) => cancelBooking(model, (user, bookingID))
  | ClearBookings => clearBookings(model)
  end
where 
  let addBooking = fun (model, (user, weekday, timeOfDay)) -> model end
  let cancelBooking = fun (model, (user, bookingID)) -> model end
  let clearBookings = fun model -> model end
  end