# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun model, action ->
  case action
  | AddSong(song) => model.addSong(song)
  | RemoveSong(song) => model.removeSong(song)
  | PlayNext => model.playNext()
  | Shuffle => model.shuffle()
  end
in