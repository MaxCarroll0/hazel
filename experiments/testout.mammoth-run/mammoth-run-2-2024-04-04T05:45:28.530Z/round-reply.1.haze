fun model, action ->
  case action
    | AddBooking(user, weekday, time) => if !bookingExists(model, (time, user, model.bookingCount + 1)) then model else addBooking(model, (time, user, model.bookingCount + 1))
    | RemoveBooking(id) => removeBooking(model, id)
    | UpdateBooking(id, time, user) => updateBooking(model, id, time, user)
  end