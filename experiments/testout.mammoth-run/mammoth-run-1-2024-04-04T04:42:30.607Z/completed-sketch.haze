# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun (model, action) ->
  case action
  | Add(song) => add_song(model, song)
  | Remove(song) => remove_song(model, song)
  | Play => play(model)
  | Pause => pause(model)
  | Stop => stop(model)
  | Next => next(model)
  | Prev => prev(model)
  end
in