fun (model, action) ->
  case action
  | AddBooking((user, day, time)) => 
      let (_, bookings, id) = model in
      ((day, time), user, id)::bookings, id + 1
  | CancelBooking((user, id)) => 
      let (formData, bookings, _) = model in
      formData, rm_booking(user, id, bookings), id - 1
  | ClearBookings => 
      initFormState, [], id
  end