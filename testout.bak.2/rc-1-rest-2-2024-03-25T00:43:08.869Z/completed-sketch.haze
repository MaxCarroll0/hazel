# Update Playlist app model based on an action #

let update: (PlayList, PlayListAction) -> PlayList =
  fun (playList, playListAction) ->
  case playListAction
  of AddSong(id) => playList.add_song(id)
   | RemoveSong(id) => playList.remove_song(id)
   | TogglePlay => playList.toggle_play() end
  end
in