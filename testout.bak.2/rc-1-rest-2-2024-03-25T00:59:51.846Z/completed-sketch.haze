# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  fun model, action ->
  case action
  | AddBooking(booking_info) => addBooking(model, booking_info)
  | CancelBooking(booking_id) => cancelBooking(model, booking_id)
  | ClearBookings => clearBookings(model)
  end
in
