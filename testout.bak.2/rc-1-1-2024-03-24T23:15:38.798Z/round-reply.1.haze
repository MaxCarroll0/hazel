fun model, action ->
  case action
  | SelectEmoji(emoji) => {model with selected_emoji = emoji}
  | Paint(point) => {model with grid = updateGrid(model.grid, point.row, point.col, model.selected_emoji)}
  | PaintFill(row) => {model with grid = fillRowInGrid(model.grid, row, model.selected_emoji)}
  | Clear => {model with grid = clearGrid(model.grid)}
  end