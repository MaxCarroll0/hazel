fun model, action ->
  case action
    | AddRoom(name) => { model with rooms = name::model.rooms }
    | RemoveRoom(name) => { model with rooms = List.remove(name, model.rooms) }
    | BookRoom(name) => { model with booked = List.append(model.booked, [name]) }
    | UnbookRoom(name) => { model with booked = List.remove(name, model.booked) }
  end