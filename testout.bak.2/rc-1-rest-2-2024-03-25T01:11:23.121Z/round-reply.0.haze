
fun model, action ->
  case action
  | SelectEmoji(emoji) => (fst model, emoji, snd model)
  | StampEmoji((row, col)) => (updateGrid(fst model, row, col, snd(fst model)), snd(fst model), snd model)
  | ClearCell((row, col)) => (updateGrid(fst model, row, col, ""), snd(fst model), snd model)
  | ClearGrid => (clearGrid(fst model), snd(fst model), snd model)
  | FillRow(row) => (fillRowInGrid(fst model, row, snd(fst model)), snd(fst model), snd model)
  end