# Update the Room Booking app model based on an action #

let update: (Model, Action) -> Model =
  
fun model, action ->
  case action
    | SetRoomNumber(s) =>
      {model with room_number = s}
    | SetNumGuests(x) =>
      {model with num_guests = x}
    | SetCheckIn(c) =>
      {model with check_in = c}
    | SetCheckOut(c) =>
      {model with check_out = c}
  end
in
