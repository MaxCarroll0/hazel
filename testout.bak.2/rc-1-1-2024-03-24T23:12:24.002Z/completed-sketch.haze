# TODO MVU: Handle TODO actions to update the app model #

let update: (Model, Action) -> Model =
  
fun model, action ->
  case action
  | addTodo => {model with todos= add(model)}
  | removeTodo(i) => {model with todos= remove(i,model.todos)}
  | toggleTodo(i) => {model with todos= toggle(i,model.todos)}
  | _ => model
  end
  in
in
