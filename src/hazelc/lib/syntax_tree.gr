export enum BinBoolOp {
    And,
    Or,
}

export enum BinIntOp {
    Plus,
    Minus,
    Times,
    Divide,
}

export enum DHExp {
    EmptyHole(Number),
    NonEmptyHole(
      Number,
      DHExp,
    ),
    Ap(DHExp, DHExp),
    BoolLit(Bool),
    BinBoolOp(BinBoolOp, DHExp, DHExp),
    BinIntOp(BinIntOp, DHExp, DHExp),
}

export enum HTyp {
  Indet(DHExp),
  Int(Number),
  Bool(Number),
}
